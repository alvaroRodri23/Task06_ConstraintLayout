<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/txtBoton"
        android:layout_width="match_parent"
        android:layout_height="62dp"
        android:background="@drawable/ic_launcher_background"
        android:text="Edit Profile"
        android:textColor="#020202"
        app:backgroundTint="#E8D5D5"
        app:icon="@android:drawable/ic_dialog_info"
        app:iconTint="#FFFFFF"
        app:strokeColor="#DC220404"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="429dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="189dp"
        android:layout_height="52dp"
        android:text="Nombre"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="377dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Followers                                  Following                        Estrellita"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="491dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="409dp"
        android:layout_height="322dp"
        app:srcCompat="@drawable/ciruclo"
        tools:layout_editor_absoluteY="53dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="411dp"
        android:layout_height="48dp"
        android:background="@color/design_default_color_primary"
        android:ems="10"
        android:inputType="text"
        android:text="  EjercicioAndroid2"
        android:textColor="#F3E4E4"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />
</androidx.constraintlayout.widget.ConstraintLayout>